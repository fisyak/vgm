<?xml version="1.0" encoding="UTF-8"?>
<gdml xmlns:gdml="http://cern.ch/2001/Schemas/GDML"
      xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      xsi:noNamespaceSchemaLocation="gdml_1.0.xsd">

   <define>
      <position  name=   "pos_0"  x="     0.0000"  y="     0.0000"  z="     0.0000"  unit="cm" />
      <position  name=   "pos_1"  x="  -125.0000"  y="     0.0000"  z="  -200.0000"  unit="cm" />
      <position  name=   "pos_2"  x="   125.0000"  y="     0.0000"  z="  -200.0000"  unit="cm" />
      <position  name=   "pos_3"  x="  -250.0000"  y="     0.0000"  z="   200.0000"  unit="cm" />
      <position  name=   "pos_4"  x="     0.0000"  y="     0.0000"  z="   200.0000"  unit="cm" />
      <position  name=   "pos_5"  x="   250.0000"  y="     0.0000"  z="   200.0000"  unit="cm" />

      <rotation  name=   "rot_0"  x="     0.0000"  y="     0.0000"  z="     0.0000"  unit="degree" />
      <rotation  name=   "rot_1"  x="     0.0000"  y="   -45.0000"  z="     0.0000"  unit="degree" />
   </define>

   <materials>
      <element  name="ALUMINIUM_e"  Z=" 13.0000"  N=" 27" >
         <atom type="A" unit="g/mol" value=" 26.9815" />
      </element>

      <material  name="Basic"  >
         <D type="density" unit="g/cm3" value="     2.7000"/>
         <fraction n="    1.0000"  ref="ALUMINIUM_e"/>
      </material>

   </materials>

   <solids>
      <box  lunit="cm" aunit="degree"
         name="worldV_s"
         x=" 2000.0000"  y=" 2000.0000"  z=" 2000.0000" />

      <box  lunit="cm" aunit="degree"
         name="volume1_s"
         x="  100.0000"  y="  100.0000"  z="  100.0000" />

      <cone  lunit="cm" aunit="degree"
         name="volume2_s"
         rmin1="   10.0000"  rmin2="   20.0000"  rmax1="   30.0000"  rmax2="   40.0000"
         z="  200.0000"
         startphi="    0.0000"  deltaphi="  360.0000" />

      <!-- !!! unsupported shape  !!!  name= "solid1Isolid2V_s" -->
      <!-- dummy box is written instead  -->
      <box  lunit="cm" aunit="degree"
         name="solid1Isolid2V_s"
         x="    0.2000"  y="    0.2000"  z="    0.2000" />

      <!-- !!! unsupported shape  !!!  name= "solid1Ssolid2V_s" -->
      <!-- dummy box is written instead  -->
      <box  lunit="cm" aunit="degree"
         name="solid1Ssolid2V_s"
         x="    0.2000"  y="    0.2000"  z="    0.2000" />

      <!-- !!! unsupported shape  !!!  name= "solid1Usolid2V_s" -->
      <!-- dummy box is written instead  -->
      <box  lunit="cm" aunit="degree"
         name="solid1Usolid2V_s"
         x="    0.2000"  y="    0.2000"  z="    0.2000" />

   </solids>

   <structure>
      <volume name="volume1">
         <materialref ref="Basic"/>
         <solidref ref="volume1_s"/>
      </volume>

      <volume name="volume2">
         <materialref ref="Basic"/>
         <solidref ref="volume2_s"/>
      </volume>

      <volume name="solid1Isolid2V">
         <materialref ref="Basic"/>
         <solidref ref="solid1Isolid2V_s"/>
      </volume>

      <volume name="solid1Ssolid2V">
         <materialref ref="Basic"/>
         <solidref ref="solid1Ssolid2V_s"/>
      </volume>

      <volume name="solid1Usolid2V">
         <materialref ref="Basic"/>
         <solidref ref="solid1Usolid2V_s"/>
      </volume>

      <volume name="worldV">
         <materialref ref="Basic"/>
         <solidref ref="worldV_s"/>
         <child>
            <volumeref ref="volume1"/>
               <positionref ref="pos_1"/>
               <rotationref ref="rot_0"/>
         </child>
         <child>
            <volumeref ref="volume2"/>
               <positionref ref="pos_2"/>
               <rotationref ref="rot_0"/>
         </child>
         <child>
            <volumeref ref="solid1Isolid2V"/>
               <positionref ref="pos_3"/>
               <rotationref ref="rot_1"/>
         </child>
         <child>
            <volumeref ref="solid1Ssolid2V"/>
               <positionref ref="pos_4"/>
               <rotationref ref="rot_1"/>
         </child>
         <child>
            <volumeref ref="solid1Usolid2V"/>
               <positionref ref="pos_5"/>
               <rotationref ref="rot_1"/>
         </child>
      </volume>

   </structure>

   <setup name="unit1"   version="1.0">
      <world ref="worldV"/>
   </setup>

</gdml>
