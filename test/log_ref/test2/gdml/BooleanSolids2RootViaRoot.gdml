<?xml version="1.0" encoding="UTF-8"?>
<gdml xmlns:gdml="http://cern.ch/2001/Schemas/GDML"
      xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      xsi:noNamespaceSchemaLocation="gdml.xsd">

   <define>
      <position  name="pos_0"     x="     0.0000"  y="     0.0000"  z="     0.0000"  unit="cm" />
      <position  name="pos_1"     x="  -125.0000"  y="     0.0000"  z="  -200.0000"  unit="cm" />
      <position  name="pos_2"     x="   125.0000"  y="     0.0000"  z="  -200.0000"  unit="cm" />
      <position  name="pos_3"     x="  -250.0000"  y="     0.0000"  z="   200.0000"  unit="cm" />
      <position  name="pos_5"     x="     0.0000"  y="     0.0000"  z="   200.0000"  unit="cm" />
      <position  name="pos_6"     x="   250.0000"  y="     0.0000"  z="   200.0000"  unit="cm" />

      <rotation  name="rot_0"     x="     0.0000"  y="     0.0000"  z="     0.0000"  unit="degree" />
      <rotation  name="rot_1"     x="     0.0000"  y="   -45.0000"  z="     0.0000"  unit="degree" />
   </define>

   <materials>

      <element  name="ALUMINIUM_e"  Z=" 13.0000"  N=" 27" >
         <atom type="A" unit="g/mol" value=" 26.9800" />
      </element>

      <material  name="Basic"  >
         <D type="density" unit="g/cm3" value="     2.7000"/>
         <fraction n="    1.0000"  ref="ALUMINIUM_e"/>
      </material>

   </materials>

   <solids>
      <box  lunit="cm" aunit="degree"
         name="world_s"
         x="  800.0000"  y="  200.0000"  z="  800.0000" />

      <box  lunit="cm" aunit="degree"
         name="solid1_s"
         x="  100.0000"  y="  100.0000"  z="  100.0000" />

      <cone  lunit="cm" aunit="degree"
         name="solid2_s"
         rmin1="   10.0000"  rmin2="   20.0000"  rmax1="   30.0000"  rmax2="   40.0000"
         z="  200.0000"
         startphi="    0.0000"  deltaphi="  360.0000" />

      <box  lunit="cm" aunit="degree"
         name="solid1Isolid2_constA_s"
         x="  100.0000"  y="  100.0000"  z="  100.0000" />

      <cone  lunit="cm" aunit="degree"
         name="solid1Isolid2_constB_s"
         rmin1="   10.0000"  rmin2="   20.0000"  rmax1="   30.0000"  rmax2="   40.0000"
         z="  200.0000"
         startphi="    0.0000"  deltaphi="  360.0000" />

      <intersection name="solid1Isolid2_s" >
         <first  ref="solid1Isolid2_constA_s" />
         <second ref="solid1Isolid2_constB_s" />
         <position  name="pos_4"     x="    20.0000"  y="     0.0000"  z="     0.0000"  unit="cm" />
         <rotation  name="rot_2"     x="   -39.2315"  y="    37.7612"  z="   -26.5651"  unit="degree" />
      </intersection>

      <box  lunit="cm" aunit="degree"
         name="solid1Ssolid2_constA_s"
         x="  100.0000"  y="  100.0000"  z="  100.0000" />

      <cone  lunit="cm" aunit="degree"
         name="solid1Ssolid2_constB_s"
         rmin1="   10.0000"  rmin2="   20.0000"  rmax1="   30.0000"  rmax2="   40.0000"
         z="  200.0000"
         startphi="    0.0000"  deltaphi="  360.0000" />

      <subtraction name="solid1Ssolid2_s" >
         <first  ref="solid1Ssolid2_constA_s" />
         <second ref="solid1Ssolid2_constB_s" />
         <position  name="pos_4"     x="    20.0000"  y="     0.0000"  z="     0.0000"  unit="cm" />
         <rotation  name="rot_2"     x="   -39.2315"  y="    37.7612"  z="   -26.5651"  unit="degree" />
      </subtraction>

      <box  lunit="cm" aunit="degree"
         name="solid1Usolid2_constA_s"
         x="  100.0000"  y="  100.0000"  z="  100.0000" />

      <cone  lunit="cm" aunit="degree"
         name="solid1Usolid2_constB_s"
         rmin1="   10.0000"  rmin2="   20.0000"  rmax1="   30.0000"  rmax2="   40.0000"
         z="  200.0000"
         startphi="    0.0000"  deltaphi="  360.0000" />

      <union name="solid1Usolid2_s" >
         <first  ref="solid1Usolid2_constA_s" />
         <second ref="solid1Usolid2_constB_s" />
         <position  name="pos_4"     x="    20.0000"  y="     0.0000"  z="     0.0000"  unit="cm" />
         <rotation  name="rot_2"     x="   -39.2315"  y="    37.7612"  z="   -26.5651"  unit="degree" />
      </union>

   </solids>

   <structure>
      <volume name="solid1">
         <materialref ref="Basic"/>
         <solidref ref="solid1_s"/>
      </volume>

      <volume name="solid2">
         <materialref ref="Basic"/>
         <solidref ref="solid2_s"/>
      </volume>

      <volume name="solid1Isolid2">
         <materialref ref="Basic"/>
         <solidref ref="solid1Isolid2_s"/>
      </volume>

      <volume name="solid1Ssolid2">
         <materialref ref="Basic"/>
         <solidref ref="solid1Ssolid2_s"/>
      </volume>

      <volume name="solid1Usolid2">
         <materialref ref="Basic"/>
         <solidref ref="solid1Usolid2_s"/>
      </volume>

      <volume name="world">
         <materialref ref="Basic"/>
         <solidref ref="world_s"/>
         <physvol>
            <volumeref ref="solid1"/>
               <positionref ref="pos_1"/>
               <rotationref ref="rot_0"/>
         </physvol>
         <physvol>
            <volumeref ref="solid2"/>
               <positionref ref="pos_2"/>
               <rotationref ref="rot_0"/>
         </physvol>
         <physvol>
            <volumeref ref="solid1Isolid2"/>
               <positionref ref="pos_3"/>
               <rotationref ref="rot_1"/>
         </physvol>
         <physvol>
            <volumeref ref="solid1Ssolid2"/>
               <positionref ref="pos_5"/>
               <rotationref ref="rot_1"/>
         </physvol>
         <physvol>
            <volumeref ref="solid1Usolid2"/>
               <positionref ref="pos_6"/>
               <rotationref ref="rot_1"/>
         </physvol>
      </volume>

   </structure>

   <setup name="unit1"   version="1.0">
      <world ref="world"/>
   </setup>

</gdml>
