<?xml version="1.0" encoding="UTF-8"?>
<gdml xmlns:gdml="http://cern.ch/2001/Schemas/GDML"
      xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      xsi:noNamespaceSchemaLocation="gdml.xsd">

   <define>
      <position  name="pos_0"     x="     0.0000"  y="     0.0000"  z="     0.0000"  unit="cm" />
      <position  name="pos_1"     x="  -450.0000"  y="  -150.0000"  z="     0.0000"  unit="cm" />
      <position  name="pos_2"     x="  -450.0000"  y="   150.0000"  z="     0.0000"  unit="cm" />
      <position  name="pos_3"     x="  -150.0000"  y="  -150.0000"  z="     0.0000"  unit="cm" />
      <position  name="pos_4"     x="     0.0000"  y="  -150.0000"  z="     0.0000"  unit="cm" />
      <position  name="pos_5"     x="     0.0000"  y="   150.0000"  z="     0.0000"  unit="cm" />
      <position  name="pos_6"     x="   150.0000"  y="   150.0000"  z="     0.0000"  unit="cm" />
      <position  name="pos_7"     x="   300.0000"  y="  -150.0000"  z="     0.0000"  unit="cm" />

      <rotation  name="rot_0"     x="     0.0000"  y="     0.0000"  z="     0.0000"  unit="degree" />
   </define>

   <materials>

      <element  name="AL_e"  Z=" 13.0000"  N="  1" >
         <atom type="A" unit="g/mol" value=" 26.9800" />
      </element>

      <material  name="Basic"  >
         <D type="density" unit="g/cm3" value="     2.7000"/>
         <fraction n="    1.0000"  ref="AL_e"/>
      </material>

   </materials>

   <solids>
      <box  lunit="cm" aunit="degree"
         name="world_s"
         x=" 1200.0000"  y="  600.0000"  z="  400.0000" />

      <box  lunit="cm" aunit="degree"
         name="box_s"
         x="   40.0000"  y="  120.0000"  z="  100.0000" />

      <cone  lunit="cm" aunit="degree"
         name="cons_s"
         rmin1="   10.0000"  rmin2="   20.0000"  rmax1="   40.0000"  rmax2="   60.0000"
         z="  100.0000"
         startphi="    0.0000"  deltaphi="  360.0000" />

      <polycone  lunit="cm" aunit="degree"
         name="pcone_s"
         startphi="    0.0000"  deltaphi="  360.0000" >
         <zplane  z="  -50.0000"  rmin="   10.0000"  rmax="   50.0000" />
         <zplane  z="   10.0000"  rmin="   20.0000"  rmax="   30.0000" />
         <zplane  z="   40.0000"  rmin="   30.0000"  rmax="   60.0000" />
      </polycone>

      <sphere  lunit="cm" aunit="degree"
         name="sphere_s"
         rmin="   20.0000"  rmax="   60.0000"
         startphi="    0.0000"  deltaphi="  360.0000"  starttheta="    0.0000"  deltatheta="  180.0000" />

      <torus  lunit="cm" aunit="degree"
         name="torus_s"
         rmin="   20.0000"  rmax="   30.0000"  rtor="   40.0000"
         startphi="    0.0000"  deltaphi="  360.0000" />

      <trd  lunit="cm" aunit="degree"
         name="trd_s"
         x1="   40.0000"  x2="   60.0000"  y1="   80.0000"  y2="  100.0000"
         z="  100.0000" />

      <tube  lunit="cm" aunit="degree"
         name="tubs_s"
         rmin="   20.0000"  rmax="   40.0000"  z="  100.0000"
         startphi="    0.0000"  deltaphi="  360.0000" />

   </solids>

   <structure>
      <volume name="box">
         <materialref ref="Basic"/>
         <solidref ref="box_s"/>
      </volume>

      <volume name="cons">
         <materialref ref="Basic"/>
         <solidref ref="cons_s"/>
      </volume>

      <volume name="pcone">
         <materialref ref="Basic"/>
         <solidref ref="pcone_s"/>
      </volume>

      <volume name="sphere">
         <materialref ref="Basic"/>
         <solidref ref="sphere_s"/>
      </volume>

      <volume name="torus">
         <materialref ref="Basic"/>
         <solidref ref="torus_s"/>
      </volume>

      <volume name="trd">
         <materialref ref="Basic"/>
         <solidref ref="trd_s"/>
      </volume>

      <volume name="tubs">
         <materialref ref="Basic"/>
         <solidref ref="tubs_s"/>
      </volume>

      <volume name="world">
         <materialref ref="Basic"/>
         <solidref ref="world_s"/>
         <physvol>
            <volumeref ref="box"/>
               <positionref ref="pos_1"/>
               <rotationref ref="rot_0"/>
         </physvol>
         <physvol>
            <volumeref ref="cons"/>
               <positionref ref="pos_2"/>
               <rotationref ref="rot_0"/>
         </physvol>
         <physvol>
            <volumeref ref="pcone"/>
               <positionref ref="pos_3"/>
               <rotationref ref="rot_0"/>
         </physvol>
         <physvol>
            <volumeref ref="sphere"/>
               <positionref ref="pos_4"/>
               <rotationref ref="rot_0"/>
         </physvol>
         <physvol>
            <volumeref ref="torus"/>
               <positionref ref="pos_5"/>
               <rotationref ref="rot_0"/>
         </physvol>
         <physvol>
            <volumeref ref="trd"/>
               <positionref ref="pos_6"/>
               <rotationref ref="rot_0"/>
         </physvol>
         <physvol>
            <volumeref ref="tubs"/>
               <positionref ref="pos_7"/>
               <rotationref ref="rot_0"/>
         </physvol>
      </volume>

   </structure>

   <setup name="unit1"   version="1.0">
      <world ref="world"/>
   </setup>

</gdml>
